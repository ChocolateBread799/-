! special
*.foreground:   #000000
*.background:   #ffffff
*.cursorColor:  #000000

*color0: #353535
*color1: #ff1010
*color2: #2e2e2e
*color3: #3d3d3d
*color4: #383838
*color5: #545454
*color6: #666666
*color7: #383838
*color8: #eeeeee
*color9: #ff1010
*color10: #2e2e2e
*color11: #3d3d3d
*color12: #383838
*color13: #545454
*color14: #666666
*color15: #ffffff

*font: xft:Roboto Mono:size=14

*lineSpace:             5
*letterSpace:           1
*geometry:              40x10
*internalBorder:        40
*scrollBar:             false


! xft fonts settings
Xft.autohint:           0
Xft.lcdfilter:          lcddefault
Xft.hinting:            1
Xft.hintstyle:          hintslight
Xft.antialias:          1
Xft.rgba:               rgb

URxvt.perl-ext-common: config-reload

URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1
